             "constraints": {
                 "fields": [
                     {
                         "path": [
                             "$.credentialSubject.associatedAddress"
                         ]
                     }
                 ]
             }
         },
    {
      "id": "over18",
      "purpose" : "Please make sure to select your Over18 proof",
      "constraints": {
          "fields": [
              {
                  "path": [
                      "$.type"
